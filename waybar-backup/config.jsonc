{
  "layer": "top",
  "modules-left": [
    "hyprland/workspaces"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    // "custom/battery",
    "custom/date",
    "custom/nettraf",
    "pulseaudio",
    "custom/memory",
    "tray",
  ],
  "hyprland/workspaces": {
    "all-outputs": true,
    "on-click": "activate",
    "format": "{icon}",
    "format-icons": {
      "1": "ó°Œ€",
      "2": "îª…",
      "3": "ó°¯ˆ",
      "4": "ó±‡",
      "5": "î˜«",
      "6": "ï‡™",
      "7": "ó±…",
      "8": "ï‘‡",
      "9": "î°›",
      "10": "ó°¡‘",
    },
  },
  "tray": {
    "icon-size": 18,
    "spacing": 6,
  },
  // "custom/battery": {
  // 	"tooltip": false,
  // 	"interval": 4,
  // 	"exec": "~/.local/bin/statusbar/battery"
  //  "return-type": "json"
  // },
  "custom/date": {
    "tooltip": false,
    "interval": 2,
    "exec": "~/.local/bin/statusbar/sb-time",
  },
  "custom/nettraf": {
    "tooltip": false,
    "interval": 4,
    "exec": "~/.local/bin/statusbar/sb-nettraf",
    "on-click-right": "kitty --class floating -e nmtui",
  },
  "pulseaudio": {
    "tooltip": false,
    "format": "{icon}{volume}%",
    "format-muted": "ğŸ”‡Muted",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "on-click-right": "pavucontrol",
    "on-scroll-up": "wpctl set-volume -l 1 @DEFAULT_AUDIO_SINK@ 5%+",
    "on-scroll-down": "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-",
    "scroll-step": 5,
    "format-icons": {
      "headphone": "ï€¥",
      "hands-free": "ï–",
      "headset": "ï–",
      "phone": "ó°·°",
      "portable": "ó°·°",
      "car": "ó°„‹",
      "default": [
        "ğŸ”ˆ",
        "ğŸ”‰",
        "ğŸ”Š"
      ],
    },
  },
  "custom/memory": {
    "tooltip": false,
    "interval": 4,
    "exec": "echo \"ğŸ§  $(free -h | awk '/Mem:/ { print $3 }' | cut -f1 -d 'i')\"",
    "on-click-right": "kitty --class floating -e htop",
  },
}